	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"seed:firebase": "node scripts/seedFirestore.cjs",
		"init:firestore": "node scripts/initFirestore.cjs",
		"cap:sync": "yarn build && npx cap sync android",
		"cap:open": "npx cap open android",
		"build:apk": "node scripts/initAppVersion.cjs && yarn build && npx cap sync android",
		"build:signed": "bash build-signed-apk.sh && git tag -a v$(node -p \"require('./package.json').version\") -m 'Release version $(node -p \"require('./package.json').version\")' && git push origin --tags",
		"release:notes": "node scripts/generateReleaseNotes.cjs"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"overrides": {
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@types/babel__helper-module-imports": "^7",
